      "name": "Authentication",
      "item": [
        {
          "name": "1. Register User",
      "event": [
        {
          "listen": "prerequest",
          "script": {
            "exec": [
              "// Generate unique email once at the beginning",
              "const timestamp = new Date().getTime();",
              "const testEmail = 'test' + timestamp + '@example.com';",
              "pm.environment.set('testEmail', testEmail);",
              "console.log('Generated email:', testEmail);"
            ]
          }
        },
        {
          "listen": "test",
          "script": {
            "exec": [
              "pm.test('Status code is 201', function() {",
              "    pm.response.to.have.status(201);",
              "});",
              "",
              "pm.test('Response has user_id and email', function() {",
              "    const jsonData = pm.response.json();",
              "    pm.expect(jsonData).to.have.property('user_id');",
              "    pm.expect(jsonData).to.have.property('email');",
              "    pm.expect(jsonData).to.have.property('message');",
              "});"
            ]
          }
        }
      ],
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"email\": \"{{testEmail}}\",\n  \"password\": \"TestPassword123\"\n}"
        },
        "url": {
          "raw": "http://localhost:8000/auth/register",
          "protocol": "http",
          "host": [
            "localhost"
          ],
          "port": "8000",
          "path": [
            "auth",
            "register"
          ]
        }
      },
      "response": []
    },
    {
      "name": "2. Login User",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "pm.test('Status code is 200', function() {",
              "    pm.response.to.have.status(200);",
              "});",
              "",
              "pm.test('Response has access_token', function() {",
              "    const jsonData = pm.response.json();",
              "    pm.expect(jsonData).to.have.property('access_token');",
              "    pm.expect(jsonData).to.have.property('token_type');",
              "    pm.expect(jsonData.token_type).to.equal('bearer');",
              "});",
              "",
              "// Save token for next requests",
              "const jsonData = pm.response.json();",
              "pm.environment.set('token', jsonData.access_token);",
              "console.log('Token saved:', jsonData.access_token.substring(0, 30) + '...');"
            ]
